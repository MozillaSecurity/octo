{
  "name": "@mozillasecurity/octo",
  "version": "3.0.0",
  "description": "A unified shared library which aids in building fuzzers for browsers or as complement for an existing fuzzing framework.",
  "keywords": [
    "fuzzing",
    "browser",
    "firefox",
    "random",
    "javascript",
    "node",
    "library"
  ],
  "homepage": "https://github.com/mozillasecurity/octo",
  "bugs": {
    "url": "https://github.com/mozillasecurity/octo/issues",
    "email": "cdiehl@mozilla.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mozillasecurity/octo.git"
  },
  "license": "MPL-2.0",
  "author": "Christoph Diehl <cdiehl@mozilla.com>",
  "main": "dist/octo.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "webpack --mode=production",
    "coverage": "cross-env NODE_ENV=test jest --silent --coverage --collectCoverageFrom=lib/**/*.ts",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "docs": "typedoc ./lib",
    "lint": "cross-env NODE_ENV=test eslint ./lib --ext ts",
    "lint:fix": "cross-env NODE_ENV=test eslint ./lib --ext ts --fix",
    "prepare": "npm run build",
    "prepush": "npm run lint && npm test",
    "test": "jest --silent",
    "watch": "webpack --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**.ts": [
      "npx prettier --write",
      "eslint --cache ./lib/ --fix"
    ],
    "package.json": "sort-package-json"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false
  },
  "dependencies": {
    "@types/html-entities": "^1.2.16",
    "@types/jsesc": "^2.5.0",
    "@types/mersenne-twister": "^1.1.2",
    "eslint-plugin-jsdoc": "^32.3.0",
    "html-entities": "^1.2.1",
    "jsesc": "^2.5.1",
    "mersenne-twister": "^1.1.0",
    "prettier": "^2.2.1",
    "ts-loader": "^6.2.1",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.9",
    "@types/jest": "^26.0.15",
    "@types/node": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.1.4",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-tsdoc": "^0.2.11",
    "husky": "^4.3.8",
    "jest": "^26.6.1",
    "lint-staged": "^10.5.4",
    "sort-package-json": "^1.49.0",
    "ts-jest": "^26.4.2",
    "typedoc": "^0.20.32",
    "webpack": "^5.25.0",
    "webpack-cli": "^4.5.0"
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
